Gen int g() {
    int i = 0;
    while (i == 0) {
        int j = 1;
        i++;
    }
    print_line((string) j);
    yield(j);
}

Proc main() {
    gen zm = g();
    print_line((string) next(zm));
    print_line((string) next(zm));
    print_line((string) next(zm));
    print_line((string) next(zm));
}